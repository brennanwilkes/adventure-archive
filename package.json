{
	"name": "adventure-archive",
	"version": "1.0.0",
	"description": "A thorntree forum archiver and searcher",
	"main": "src/app.js",
	"scripts": {
		"start": "node .",
		"scrapeData": "./src/dev/scrapeData.sh",
		"rootIndex": "./src/dev/rootIndexer.sh > data/forums.csv",
		"speedTest": "./src/dev/speedTest.sh https://www.lonelyplanet.com/thorntree/forums/asia-north-east-asia/japan/coronavirus-and-travel-in-japan 120",
		"dev:full": "run-p watch dev",
		"dev": "nodemon .",
		"watch": "webpack --config src/webpack.config.js --watch",
		"build:production": "sed -i 's/development/production/' src/webpack.config.js ; npm run build:frontend ; sed -i 's/production/development/' src/webpack.config.js ;",
		"build:frontend": "webpack --config src/webpack.config.js",
		"test": "jest --colors --verbose",
		"lint": "eslint \"src/**/*.js\" --fix",
		"doc": "rm -rf documentation/ ; jsdoc -d documentation/ src/app.js src/backend/server.js src/*/*/*.js src/*/*/*/*.js src/frontend/App.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brennanwilkes/adventure-archive.git"
	},
	"author": "Brennan Wilkes",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/brennanwilkes/adventure-archive/issues"
	},
	"homepage": "https://github.com/brennanwilkes/adventure-archive#readme",
	"dependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"axios": "^0.21.0",
		"babel-loader": "^8.1.0",
		"bootstrap": "^5.0.0-alpha2",
		"css-loader": "^5.0.0",
		"dotenv": "^8.2.0",
		"expres": "0.0.5",
		"express": "^4.17.1",
		"express-validator": "^6.7.0",
		"jquery": "^3.5.1",
		"moment": "^2.29.1",
		"mongoose": "^5.10.17",
		"node": "^14.13.0",
		"popper.js": "^1.16.1",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-icons": "^3.11.0",
		"shasum": "^1.0.2",
		"style-loader": "^2.0.0",
		"unescape": "^1.0.1"
	},
	"devDependencies": {
		"eslint": "^7.14.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.21.5",
		"jest": "^26.6.3",
		"jest-junit": "^12.0.0",
		"jsdoc": "^3.6.6",
		"nodemon": "^2.0.6",
		"npm-run-all": "^4.1.5",
		"supertest": "^6.0.1",
		"webpack": "^5.3.2",
		"webpack-cli": "^4.1.0"
	}
}
